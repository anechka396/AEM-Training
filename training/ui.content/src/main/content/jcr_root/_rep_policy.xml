<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:rep="internal"
    jcr:primaryType="rep:ACL">
    <allow
        jcr:primaryType="rep:GrantACE"
        rep:principalName="administrators"
        rep:privileges="{Name}[jcr:all]"/>
    <allow0
        jcr:primaryType="rep:GrantACE"
        rep:principalName="contributor"
        rep:privileges="{Name}[jcr:read]"/>
    <allow1
        jcr:primaryType="rep:GrantACE"
        rep:principalName="version-purge-service"
        rep:privileges="{Name}[jcr:versionManagement,jcr:read]"/>
    <deny
        jcr:primaryType="rep:DenyACE"
        rep:principalName="version-purge-service"
        rep:privileges="{Name}[jcr:versionManagement,jcr:read]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            rep:glob="libs/*/config/*"/>
    </deny>
    <deny0
        jcr:primaryType="rep:DenyACE"
        rep:principalName="version-purge-service"
        rep:privileges="{Name}[jcr:versionManagement,jcr:read]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            rep:glob="libs/*/install/*"/>
    </deny0>
    <deny1
        jcr:primaryType="rep:DenyACE"
        rep:principalName="version-purge-service"
        rep:privileges="{Name}[jcr:versionManagement,jcr:read]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            rep:glob="apps/*/config/*"/>
    </deny1>
    <deny2
        jcr:primaryType="rep:DenyACE"
        rep:principalName="version-purge-service"
        rep:privileges="{Name}[jcr:versionManagement,jcr:read]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            rep:glob="apps/*/install/*"/>
    </deny2>
    <allow2
        jcr:primaryType="rep:GrantACE"
        rep:principalName="testuser"
        rep:privileges="{Name}[jcr:read,jcr:addChildNodes,jcr:nodeTypeManagement]"/>
</jcr:root>
