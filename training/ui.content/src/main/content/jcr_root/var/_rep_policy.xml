<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:crx="http://www.day.com/crx/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:rep="internal"
    jcr:primaryType="rep:ACL">
    <deny
        jcr:primaryType="rep:DenyACE"
        rep:principalName="everyone"
        rep:privileges="{Name}[jcr:all]"/>
    <allow
        jcr:primaryType="rep:GrantACE"
        rep:principalName="administrators"
        rep:privileges="{Name}[jcr:all]"/>
    <allow0
        jcr:primaryType="rep:GrantACE"
        rep:principalName="communities-user-admin"
        rep:privileges="{Name}[jcr:read]"/>
    <allow1
        jcr:primaryType="rep:GrantACE"
        rep:principalName="testuser"
        rep:privileges="{Name}[jcr:removeChildNodes,jcr:removeNode,jcr:versionManagement,jcr:modifyAccessControl,jcr:modifyProperties,crx:replicate,jcr:readAccessControl,jcr:lockManagement]"/>
</jcr:root>
